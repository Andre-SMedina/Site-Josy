<div class="mystyle container border border-dark">
  <h2 class="text-center mb-5">Leituras Concluídas</h2>
  <div class="row mt-3">
    <div class="col-4">
      <h4>Livro</h4>
    </div>
    <div class="col">
      <h4>Início</h4>
    </div>
    <div class="col">
      <h4>Fim</h4>
    </div>
    <div class="col">
      <h4>Nota</h4>
    </div>
  </div>
  <hr class="my-2" />
  {{#each leiturasTerminadas}}
    <div class="row">
      <div class="col-4">
        <p>{{livro}}</p>
      </div>
      <div class="col">
        <p>{{dateIni}}</p>
      </div>
      <div class="col">
        <p>{{dateFim}}</p>
      </div>
      <div class="col">
        <p class="livros" id="{{livro}}">
          <i class="star bi bi-star" id="st1"></i>
          <i class="star bi bi-star" id="st2"></i>
          <i class="star bi bi-star" id="st3"></i>
        </p>
      </div>
    </div>
  {{/each}}
</div>